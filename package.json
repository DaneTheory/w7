{
  "name": "w7",
  "version": "0.0.2",
  "description": "server the pure htmls, Updates the browser on changes.",
  "repository": {
    "url": "ULIVZ/dev-server",
    "type": "git"
  },
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "xo",
    "prepublish": "npm run build",
    "build": "bili src/{index,cli}.js --filename [name].js --js babel --no-babel.babelrc"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "cac": "^4.4.4",
    "chalk": "^2.3.2",
    "connect": "^3.6.6",
    "connect-livereload": "^0.6.0",
    "livereload": "^0.7.0",
    "open": "0.0.5",
    "rollup-plugin-babel": "^3.0.3",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "bili": "3.0.4",
    "eslint-config-rem": "^3.0.0",
    "jest-cli": "^19.0.0",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": "rem",
    "envs": [
      "jest"
    ]
  }
}
